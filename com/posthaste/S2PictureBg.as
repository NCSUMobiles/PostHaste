package {		import flash.display.MovieClip;	import fl.display.ProLoader;	import flash.media.MediaPromise;	import flash.display.Bitmap;		public class S2PictureBg extends MovieClip {		var rsvpIndex:uint;		var loader:ProLoader;		var mediaPromise:MediaPromise;		var fileLoc:String;		var bitMap:Bitmap;				public function S2PictureBg(rsvpIndex:uint=0, mediaPromise:MediaPromise=null, loader:ProLoader=null) {			this.rsvpIndex = rsvpIndex;			//setMediaPromise(mediaPromise);			setLoader(loader);		}				public function setMediaPromise(mediaPromise:MediaPromise):void {			this.mediaPromise = mediaPromise;			if (this.mediaPromise == null) {				trace("Null MediaPromise");				// Might need to do something else here.				return;			}			this.fileLoc = this.mediaPromise.file.url;			trace("MediaPromise: "+ this.mediaPromise.file.url);			// TODO: Possibly setup the loader, too.		}				public function setLoader(loader:ProLoader) {			this.loader = loader;			if (this.loader == null) {				// Consider deleting the child loader?				return;			}			this.loader.x = this.loader.y = 0;			this.loader.width = this.width;			this.loader.height = this.height;			bitMap = this.loader.content as Bitmap;			addChild(this.loader);		}	}	}